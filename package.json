{
  "name": "web-dev-exam",
  "version": "0.0.1",
  "description": "Web app made for Web development course @ uniandes (Fall 2018)",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "heroku-postbuild": "cd frontend && npm install && npm run build"
  },
  "keywords": [
    "Mongo",
    "React"
  ],
  "author": "Juli√°n Manrique (ja.manrique@uniandes.edu.co)",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "mongodb": "^3.1.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-console": 0
    }
  }
}
